<Project Sdk="WixToolset.Sdk/4.0.5">
  <PropertyGroup>
    <OutputType>Package</OutputType>
    <OutputName>TabScore2</OutputName>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="WixToolset.UI" Version="4.0.5" />
    <PackageReference Include="WixToolset.Heat" Version="4.0.5" />
  </ItemGroup>
  <ItemGroup>
    <HarvestDirectory Include="..\TabScore2\publish">
      <ComponentGroupName>ProductComponents</ComponentGroupName>
      <DirectoryRefId>INSTALLFOLDER</DirectoryRefId>
      <SuppressCom>true</SuppressCom>
      <SuppressRegistry>true</SuppressRegistry>
      <SuppressRootDirectory>true</SuppressRootDirectory>
      <PreprocessorVariables>var.publishDir=..\TabScore2\publish</PreprocessorVariables>
    </HarvestDirectory>
  </ItemGroup>
  <ItemGroup>
    <Content Include="TabScore2.wxs" />
  </ItemGroup>
</Project>
